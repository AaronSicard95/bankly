{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email",
        "phone"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "Jayson"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "$5eieifoi3ioj3oi"
            ]
        },
        "first_name": {
            "type": "string",
            "default": "",
            "title": "The first_name Schema",
            "examples": [
                "Jayson"
            ]
        },
        "last_name": {
            "type": "string",
            "default": "",
            "title": "The last_name Schema",
            "examples": [
                "Bigalo"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "Jayson.Bigalo@gmail.com"
            ]
        },
        "phone": {
            "type": "integer",
            "default": "",
            "title": "The email Schema",
            "examples": [
                7765543322
            ]
        },
        "admin": {
            "type": "bool"
        }
    },
    "examples": [{
        "username": "Jayson",
        "password": "$5eieifoi3ioj3oi",
        "first_name": "Jayson",
        "last_name": "Bigalo",
        "email": "Jayson.Bigalo@gmail.com",
        "phone": 7765543322
    }]
}